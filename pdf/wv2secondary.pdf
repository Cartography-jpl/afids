procedure	! wv2secondary.pdf

parm fname string
parm refimg string
parm elevdata string

local img string init=".img"
local ntf string init=".NTF"
local found integer

local afidsvdevdata string
!local elevdata string

body

write "wv2secondary version Sat May  7 2011"

write "fname &fname"
write "refimg &refimg"
write "elevdata &elevdata"

tcl2tcl "puts time_wv2secondary_begins[exec date]"

file2tcl &fname&img val=found
if (found=1) goto doadbands

!translog AFIDS_VDEV_DATA afidsvdevdata
!let elevdata = "&afidsvdevdata/middle_east_elv.hlf"

ikqbcall2 key=tst +
     rawimg=&fname&ntf +
     rawmeta="" rawhdr="" +
     outimg=&fname&img +
     refimg=&refimg&img +
     nah=950 nav=950 rtype=secondary +
     dted="&elevdata" +
     base="" rawtype=nitf senstype=q +
     mpix=0.5 mpixf=1.0 maptype=pc xvdonly=n +
     outrpc="" outrpctp="ntf" +
     linesamp=(-999.0,-999.0,-999.0,-999.0,,) +
     lsatls=(-999.0,-999.0,-999.0,-999.0,,) +
     usermapref="" interp=bilin rastype=area +
     siteref="" siteout="" +
     line_upper="" line_lower="" samp_left="" samp_right=""

doadbands>

wv2addbands &fname &refimg "&elevdata"

tcl2tcl "puts time_wv2secondary_ends[exec date]"

end-proc
