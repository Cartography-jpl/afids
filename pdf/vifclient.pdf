procedure help=*
parm ip       type=(string,99) count=1
parm port     type=(string,99) count=1
parm filename type=(string,99) count=1
body

let _onfail="continue"

!local afidsroot type=(string,128)
!local osname type=(string,20)
!translog AFIDS_ROOT afidsroot
!ush &afidsroot/../bin/vifclient &ip &port &filename
tush vifClient &ip &port &filename

end-proc

