   geotiff=("ModelTiePointTag=(2,3,0,30000.,17000.,0.0)", +
          "ModelTiePointTag=(202,103,0,31000.,16000.,0.0)", +
          "ModelTiePointTag=(202,3,0,31000.,17000.,0.0)", +
          "GTRasterTypeGeoKey=1(RasterPixelIsArea)")
