mf3 xxxa +
 f="c1=118+@rand$c2=35+@rand$c3=500*(@rand+6)$c4=500*@rand+10$c5=800*@rand$c6=0"

rpcsccub xxxa rpctype=b
ibis-l xxxa cfor="%7.2f %6.2f %7.2f %7.2f %7.2f %13.8f"

ibis-copy xxxa xxxb
rpcscale (xxxa,xxxb) rpccol=6
ibis-l xxxb cfor="%7.2f %6.2f %7.2f %7.2f %7.2f %13.8f"

end-proc
