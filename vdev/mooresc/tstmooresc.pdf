procedure
local tline int
local tsamp int

refgbl $echo
refgbl $autousage

! test script for mooresc
!  10-06-2019 - rjb
!
! vicar programs
!       gen f2 list
!
! external programs
!       <none>
!
body
let $autousage="none"
let _onfail="stop"
let $echo="yes"

! TEST CASE FOR mooresc

gen xxxim1 nl=10 ns=10 sinc=0
gen xxxim2 nl=10 ns=10 linc=0
f2 inp=(xxxim1,xxxim2) out=xxxim3 +
 func="2*((abs(in1-5)+abs(in2-5))<2)+3*((abs(in1-3)+abs(in2-6))<2)"
list xxxim3 'zer

mooresc xxxim3 xxxim4 dmax=2 pixval=(2,3) numpix=2

list xxxim4 'zer

mooresc xxxim3 (xxxim4,xxxim5) dmax=2 pixval=(2,3) numpix=2

list xxxim4 'zer
list xxxim5 'zer

let $echo="no"
theend>
END-PROC
