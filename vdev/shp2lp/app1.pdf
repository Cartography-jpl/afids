lpscrb inp=(lpTest2.slp) out=someimage spacing=1 sline=1 ssamp=1 nline=3674 nsamp=5295 autozoom=0 nolong=0.0 dn=255 bdn=0 pdn=0 outside=0 tag=0 refl=0
