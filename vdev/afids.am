# long file) or in separate .am files in each subdirectory (making tons of 
# very short files). For now put everything here, and we can change this
# later if I change my mind.

# This testing uses GNU Makefile specific functions. It would be too complicated
# take make this work with all Makefiles, and at least for now we can assume we
# have GNU make whenever we are doing tests.
#  
#  8-11-2013 - Ray Bambery - Added certain comments to facilitate the git Cookbook
#  4-19-2014 - added TIFF_LIBS to vtiff3 to build under MacOSX 10.8.5/fink gcc 4.8.2 compiler
#
# SECTION 0 - Check Environment
# =======================================================================
afids_CHECK = 
afids_P2_CHECK =

TESTS_ENVIRONMENT = 
TESTS += $(afids_CHECK) $(afids_P2_CHECK)
check_PROGRAM+= $(afids_CHECK)
check_DATA+= $(patsubst %.diff, %.out, $(afids_CHECK))
check_DATA+= $(patsubst %.diff, %.p2_out, $(afids_P2_CHECK))

check: check_clean

check_clean:
	-rm -r Test

CLEAN_HOOK += check_clean

# Run test in a scratch area (so temporary files don't interfere with other
# tests). We use tput to color the output for terminals that support it.
# To see if your terminal supports color in unix enter "msgcat --color=test"
# setaf (foreground) color codes: 0=black,1=red,2=green,3=yellow,4=blue,5=purple,6=cyan,7=white,
# 9=default color as foreground color
red = $$(tput setaf 1)
green = $$(tput setaf 2)
blue = $$(tput setaf 4)
default = $$(tput sgr0)

# Valgrind can optionally be used
VALGRIND_PROG = $(if $(VALGRIND), valgrind --track-origins=yes --trace-children=yes --error-exitcode=1  --max-stackframe=8261912)

# We set up the data in 2 ways, depending on if we are installing this as
# part of the build or not
if BUILD_AFIDS_DATA
afids_data_setup = export LANDSAT_ROOT="$(landsatdir)" && export ELEV_ROOT="$(srtml2dir)" && export CIB1_ROOT="$(cib01dir)" && export CIB5_ROOT="$(cib05dir)" && export SPICEDATA="$(datacspice)" && export AFIDS_VDEV_DATA="$(abs_builddir)/afids_data/data/vdev"
else
afids_data_setup = source $(AFIDS_DATA)/setup_afids_data.sh
endif

# Similarly, we get the taetm program in 2 ways depending on if we are building
# this or not
if BUILD_VICAR
taetm_prog = $(abs_builddir)/taetm
else
taetm_prog = $(VICAR_PREFIX)/vicar/tae/bin/taetm
endif

# Have a bunch of environment variables to set up before doing a test.
if BUILD_VICAR
test_setup =  export ELLIPSOID_DATA="$(abs_srcdir)/$(srcgeotrans)" && \
        export DATUM_DATA="$(abs_srcdir)/$(srcgeotrans)" && \
        export TAEPDF="$(abs_builddir)" && \
        export GNUPLOT="$(GNUPLOT_ABS)" && \
        $(afids_data_setup) && \
	export TAE_PATH="$(abs_builddir):$(test_top_dir)/$*:$(abs_srcdir)/$(srcvicarlib):$(abs_srcdir)/$(srcvidslib)" && \
        export AFIDS_ROOT="$(abs_builddir)" && \
        export PATH="$(bindir):${PATH}" && \
        export LD_LIBRARY_PATH="$(DESTDIR)$(libdir):$(GNUPLOT_PREFIX)/lib:$(GNUPLOT_PREFIX)/lib64:${LD_LIBRARY_PATH}" && \
        ulimit -s 32768 
else
test_setup =  export ELLIPSOID_DATA="$(abs_srcdir)/$(srcgeotrans)" && \
        export DATUM_DATA="$(abs_srcdir)/$(srcgeotrans)" && \
        export TAEPDF="$(VICAR_PREFIX)/vicar/tae/pdf" && \
        export GNUPLOT="$(GNUPLOT_ABS)" && \
        $(afids_data_setup) && \
	export TAE_PATH="$(abs_builddir):$(test_top_dir)/$*:$(VICAR_PREFIX)/vicar/bin:$(VICAR_PREFIX)/vicar/lib:$(VICAR_PREFIX)/vicar/vids/lib" && \
        export AFIDS_ROOT="$(abs_builddir)" && \
        export PATH="$(bindir):${PATH}" && \
        export LD_LIBRARY_PATH="$(DESTDIR)$(libdir):$(GNUPLOT_PREFIX)/lib:$(GNUPLOT_PREFIX)/lib64:${LD_LIBRARY_PATH}" && \
        ulimit -s 32768 
endif
# Location of test data
export AFIDS_TESTDATA=@afidstestdatadir@

# Special backdoor to hardcode the user and date/time strings that appear
# in the history of labels. Used to make comparisons easier, 
# regardless of who is actually running the tests, and at what time it is run
# the history information will be the set to these values.
export AFIDS_FAKE_TEST_USER=afids_test_user
export AFIDS_FAKE_TEST_DAT_TIM=Sat Jan  1 00:00:00 2000

test_top_dir := $(shell cd $(abs_srcdir)/$(srcafids)/; pwd)
abs_script_dir := $(shell cd $(abs_srcdir)/$(srcscript)/; pwd)

# Note down below we use the V2LIB to point to our AFIDS. This isn't really
# the V2LIB, but the slogon.pdf script hardwires things to look in the 
# $AFIDS_ROOT/vdev directory, and we don't actually have that directory yet.
Test/%.out : Test/test_data 
	@-rm -r Test/$*scratch
	@$(MKDIR_P) Test/$*scratch
	@cd Test/$*scratch; \
          for i in $(test_top_dir)/$*/*.dat; do \
            ln -s $$i . ;\
          done; \
          for i in $(test_top_dir)/$*/chk*; do \
            ln -s $$i . ;\
          done; \
          for i in $(test_top_dir)/$*/tst*; do \
            ln -s $$i . ;\
          done
	@echo "${blue}Starting test $*${default}"
	@echo "procedure" > Test/$*scratch/tst.pdf
	@echo "body" >> Test/$*scratch/tst.pdf
	@echo "enable-log" >> Test/$*scratch/tst.pdf
	@echo "tst$*" >> Test/$*scratch/tst.pdf
	@echo "disable-log" >> Test/$*scratch/tst.pdf
	@echo "end-proc" >> Test/$*scratch/tst.pdf
	@cd Test/$*scratch && \
        $(test_setup) && \
        ( $(VALGRIND_PROG) $(taetm_prog) -s ./tst.pdf > ../$*.log 2>&1 ) && echo "${green}Test run $* suceeded${default}" && exit 0; echo "${red}Test run $* failed${default}"
	@mv Test/$*scratch/session.log Test/$(@F) 

Test/%.p2_out : Test/test_data 
	@-rm -r Test/$*scratch
	@$(MKDIR_P) Test/$*scratch
	@cd Test/$*scratch; \
          for i in $(test_top_dir)/p2tests/*.dat; do \
            ln -s $$i . ;\
          done; \
          for i in $(test_top_dir)/p2tests/chk*; do \
            ln -s $$i . ;\
          done; \
          for i in $(test_top_dir)/p2tests/tst*; do \
            ln -s $$i . ;\
          done
	@echo "${blue}Starting test $*${default}"
	@echo "procedure" > Test/$*scratch/tst.pdf
	@echo "body" >> Test/$*scratch/tst.pdf
	@echo "enable-log" >> Test/$*scratch/tst.pdf
	@echo "tst$*" >> Test/$*scratch/tst.pdf
	@echo "disable-log" >> Test/$*scratch/tst.pdf
	@echo "end-proc" >> Test/$*scratch/tst.pdf
	@cd Test/$*scratch && \
        $(test_setup) && \
        ( $(VALGRIND_PROG) $(taetm_prog) -s ./tst.pdf > ../$*.log 2>&1 ) && echo "${green}Test run $* suceeded${default}" && exit 0; echo "${red}Test run $* failed${default}"
	@mv Test/$*scratch/session.log Test/$(@F) 

Test/test_data : 
	$(MKDIR_P) Test
	$(LN_S) @afidstestdatadir@ Test/test_data > /dev/null 2>&1

Test/%.diff : $(srcafids)/%/expected_test_results/tst.out
	echo "$(abs_script_dir)/tst_compare.sh Test/$*.out $(test_top_dir)/$*/expected_test_results/tst.out > Test/$*.diff_result || exit 1" > $@
	if test -n "$(VALGRIND)"; then \
           echo "grep ERROR Test/$*.log | grep -v 'ERROR SUMMARY: 0 errors' >> Test/$*.diff_result && exit 1" >> $@; \
        fi
	chmod +x $@

Test/%.diff : $(srcafids)/p2tests/expected_test_results/tst%.out
	echo "$(abs_script_dir)/tst_compare.sh Test/$*.p2_out $(test_top_dir)/p2tests/groundtruth/tst$*.out > Test/$*.diff_result || exit 1" > $@
	if test -n "$(VALGRIND)"; then \
           echo "grep ERROR Test/$*.log | grep -v 'ERROR SUMMARY: 0 errors' >> Test/$*.diff_result && exit 1" >> $@; \
        fi
	chmod +x $@


